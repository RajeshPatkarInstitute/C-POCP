#include "stdafx.h"
using namespace System;

void main()
{
	Console::Clear();
	Console::SetCursorPosition(5, 5);
	Console::Write("*");
	Console::SetCursorPosition(5, 6);
	Console::Write("*");
	Console::SetCursorPosition(5, 7);
	Console::Write("*");
	Console::SetCursorPosition(5, 8);
	Console::Write("*");
	Console::ReadLine();
}
